//Funkcija koja racuna apsolutnu vrednost
function int abs(int broj) {
  int x;
  if(broj < 0){
    x = 0 - broj; //negativan broj
  }
  else
  {
    x = broj;     //pozitivan broj
  }
  return x;
}

